#include <stdio.h>
int main(){
    int L1, L2, L3 ;
printf("me de os lados");
printf("\n lado 1");
scanf( "%d", &L1);

printf("lado 2");
scanf( "%d", &L2);

printf("lado 3");
scanf( "%d", &L3);

if ( L1+L2>L3 && L2+L3>L1 && L1+L3>L2){


if (L1==L2 && L2==L3) {
    printf("EQUILATERO");
}
 else if (L2==L1 && L3!=L1 || L2==L3 && L2!=L1  ){
    printf("ISOCELES");
}

else if (L1!=L2 && L2!=L3 ){
    printf("ESCALENO");
}
}
else { printf("isso n eh um triangulo");}
}
